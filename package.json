{
  "name": "conditionalclobber",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --mode web",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "deploy:dev": "npm run build && surge ./dist https://condc-test.surge.sh"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-scroll-lock": "^2.6.1",
    "chart.js": "^2.8.0",
    "css": "^2.2.4",
    "js-base64": "^2.5.1",
    "jsonq": "^1.2.0",
    "lodash": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.6",
    "vue-async-computed": "^3.6.1",
    "vue-chartjs": "^3.4.2",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-nested-mutations": "^1.0.2",
    "vuex-persistedstate": "^2.5.4",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-plugin-unit-mocha": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "google-fonts-webpack-plugin": "^0.4.4",
    "node-sass": "^4.9.0",
    "raw-loader": "^2.0.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21",
    "workerize-loader": "^1.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "rules": {
      "no-param-reassign": "off",
      "import/prefer-default-export": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
